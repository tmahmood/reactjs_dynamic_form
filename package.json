{
  "name": "reactjs_dynamic_form",
  "version": "0.0.32",
  "description": "A dynamic form generator for reactjs ",
  "main": "./dist/formfields.js",
  "types": "./dist/formfields.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmahmood/reactjs_dynamic_form.git"
  },
  "keywords": [
    "form",
    "reactjs",
    "typescript",
    "dynamic"
  ],
  "jest": {
    "verbose": true,
    "transform": {
      "libs/__tests__/^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "Tarin Mahmood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmahmood/reactjs_dynamic_form/issues"
  },
  "homepage": "https://github.com/tmahmood/reactjs_dynamic_form#readme",
  "dependencies": {
    "react-select": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-select": "^2.0.11",
    "@types/reactstrap": "^7.1.1",
    "babel-jest": "^22.4.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.4.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "reactstrap": "^5.0.0-beta.3",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.5.0",
    "typescript": "^3.3.1"
  }
}
